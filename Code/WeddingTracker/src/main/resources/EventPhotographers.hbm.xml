<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="main.java.com.defrainphoto.weddingtracker.model.EventPhotographers" table="EVENT_PHOTOGRAPHERS" >
        <composite-id >
        	<key-many-to-one name="eventId" class="main.java.com.defrainphoto.weddingtracker.model.Event" column="EVENTID"/>
        	<key-many-to-one name="staffId" class="main.java.com.defrainphoto.weddingtracker.model.Photographer" column="STAFFID"/>
        </composite-id>
        
        <property name="isMainPhotographer">
            <column name="ISMAINPHOTOGRAPHER"/>
        </property>
    </class>
</hibernate-mapping>