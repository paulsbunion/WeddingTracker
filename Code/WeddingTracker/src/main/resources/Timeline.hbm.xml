<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="main.java.com.defrainphoto.weddingtracker.model.Timeline" table="TIMELINE">
        <id name="timelineId" column="TIMELINEID">
            <generator class="identity"/>
        </id>

		<many-to-one name="timeChunks" class="main.java.com.defrainphoto.weddingtracker.model.TimeChunk" insert="false" update="false">
			<column name="CHUNKID"></column>
			<column name="TIMELINEID" ></column>
		</many-to-one>
        
        <property name="startTime">
            <column name="STARTTIME"/>
        </property>
        
        <property name="totalTime">
            <column name="TOTALTIME"/>
        </property>
        
    </class>
</hibernate-mapping>